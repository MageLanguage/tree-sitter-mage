{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "mage",
      "camelcase": "Mage",
      "title": "Mage",
      "scope": "source.mg",
      "file-types": [
        "mg"
      ],
      "injection-regex": "^mage$",
      "class-name": "TreeSitterMage"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Mage grammar for tree-sitter",
    "authors": [
      {
        "name": "Alexander Kosachev",
        "email": "kosachev_alex@mail.ru",
        "url": "https://gohryt.online/"
      }
    ],
    "links": {
      "repository": "https://github.com/MageLanguage/tree-sitter-mage"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
